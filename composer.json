{
    "name": "kodekeep/laravel-ssh",
    "description": ":package_description",
    "keywords": [
        "kodekeep",
        "laravel-ssh"
    ],
    "homepage": "https://github.com/kodekeep/laravel-ssh",
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "brian@kodekeep.com",
            "homepage": "https://kodekeep.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "kodekeep/process-runner": "^1.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "graham-campbell/analyzer": "^2.4",
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.3",
        "spatie/phpunit-snapshot-assertions": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "KodeKeep\\SecureShell\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KodeKeep\\SecureShell\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test:coverage": "vendor/bin/phpunit --coverage-html=.coverage --coverage-clover=clover.xml",
        "format": "vendor/bin/php-cs-fixer fix ./"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "KodeKeep\\SecureShell\\SecureShellServiceProvider"
            ],
            "aliases": {
                "SecureShell": "KodeKeep\\SecureShell\\SecureShellFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
